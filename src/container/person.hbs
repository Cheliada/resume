<div>
    {{#with person}}
    <div>
        <h1>{{name}}</h1>
        <h2>Age: {{age}}</h2>
        <h3>Gender: {{gender}}</h3>
        <br>
        <hr>

        <section>
            <ins>
                {{> title data="My address:" big=true}}
            </ins>
            {{#with address big=true}}
            <address>{{street}}</address>
            <address>{{city}}</address>
            <address>{{state}}</address>
            <address>{{zip}}</address>
            <address>{{country}}</address>
            {{/with}}
        </section>
        <hr>
        <section>
            <ins>
                {{> title data="My educations" big=true}}
            </ins>
            <ul>
                {{#each education}}
                <li>
                    <p><b>Degree:</b> {{degree}}</p>
                </li>
                <li>
                    <p><b>Major:</b> {{major}}</p>
                </li>
                <li>
                    <p><b>University:</b> {{university}}</p>
                </li>
                <li>
                    <p><b>GraduationYear:</b> {{graduationYear}}</p>
                </li>
                {{/each}}
            </ul>
        </section>
        <hr>
        <section>
            <ul>
                {{#each workExperience}}
                <li>
                    <div><b>{{company}}</b></div>
                    <br>
                    <div>
                        <ins>
                            <h2>{{title}}</h2>
                        </ins>
                    </div>

                    <div>
                        {{startDate}} - {{endDate}}
                    </div>
                    <br>
                    <div>
                        <ol>
                            <p>Developed new features for Google Maps</p>
                        </ol>
                        <ol>
                            <p>Worked on improving search algorithms</p>
                        </ol>
                    </div>

                    <div>
                        <b>{{industry}}</b>

                        {{#if year_founded includeZero=true}} ({{year_founded}}) {{/if}}
                    </div>
                    <br>


                    <section>
                        <summary><ins><b>Employees list :</b></ins></summary>
                    </section>

                    {{#each employees}}
                <li>
                    <h1><b>Name:</b><ins>{{name}}</ins></h1>
                    <div><b>Position:</b><ins>{{position}}</ins></div>
                    <div><b>Department:</b><ins>{{department}}</ins></div>
                </li>
        </section>

        <section>
            <summary><ins><b>Projects :</b></ins></summary>

            {{#each projects}}


            <li>
                <h1><b>Name:</b><ins>{{name}}</ins></h1>
                <div><b>Description:</b><ins>{{description}}</ins></div>
                <div><b>Status:</b><ins>{{status}}</ins></div>
            </li>


            <br>
            <summary><ins><b>Teams :</b></ins></summary>
            <br>
            {{#each teams}}
            <li>
                <b>Team_name:</b> <ins>{{team_name}}</ins>
            </li>
            <br>
            {{#with team_leader}}
            <div><b>Name: </b> <ins>{{name}}</ins></div>
            <div><b>Title: </b> <ins>{{title}}</ins></div>
            <div><b>Email: </b> <ins>{{email}}</ins></div>
            {{/with}}
            <br>
            {{#each team_members}}
            <div><b>Name: </b> <ins>{{name}}</ins></div>
            <div><b>Position: </b> <ins>{{title}}</ins></div>
            <div><b>Email: </b> <ins>{{email}}</ins></div>
            <div><b>Skills: </b></div>
            {{#each skills}}
            <li>{{this}}</li>
            {{/each}}
            <br>
            <summary><ins><b>Projects :</b></ins></summary>
            <br>
            {{#each projects}}
            <div><b>Name: </b> <ins>{{name}}</ins></div>
            <div><b>Description: </b> <ins>{{description}}</ins></div>
            <br>
            <div><b>Technologies: </b></div>
            {{#each technologies}}
            <li>{{this}}</li>
            {{/each}}
            <br>
            <div><b>Team_members: </b></div>
            {{#each team_members}}
            <li>{{name}}</li>
            <li>{{title}}</li>
            {{/each}}
            {{/each}}




            {{/each}}
            {{/each}}

            {{/each}}
            <br>






            {{/each}}
        </section>
        </li>
        {{/each}}
        </ul>
        </section>
    </div>
    {{/with}}
</div>