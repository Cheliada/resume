{{#with service}}
<div class="row my-3 mx-3">
    <div class="col-12">
        <div class="row">
            <div class="card bg-success ">
                <div class="card-body text-center my-3">
                    <div class="row">
                        <div class="col-9">
                            <div class="text-light text-center">
                                <h2 class="text-center">{{title}}</h2>
                                <pclass="text-center">{{description}}</p>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-4">
                            <div class="row">
                            </div>
                        </div>
                        <div class="col-3">
                            <div class="row">
                                {{#each buttons}}
                                <a type="button" class="btn btn-primary text-center" href="{{link.rel}}">
                                    {{text}}
                                </a>
                                {{/each}}
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
{{/with}}