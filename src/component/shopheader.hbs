    <div class="row">
        <div class="col-4">
        </div>
        <div class="nav nav-tabs">
            {{#each links}}
            <div class="nav-item disabled">
                <a class="nav-link" href="{{href}}">{{text}}</a>
            </div>
            {{/each}}
        </div>
    </div>
</section>
{{/with}}